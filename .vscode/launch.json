{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Migrate",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/main.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "args": ["migrate"],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Apply test_schema",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/main.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "args": ["apply", "test_schema"],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Apply test_schema2",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/main.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "args": ["apply", "test_schema2"],
            "outputCapture": "std"
        }
    ]
}